{
    "name": "json-rpc-lib/json-rpc-lib",
    "description": "JSON-RPC Server/Client for PHP 5.3",
    "keywords": ["json-rpc", "rpc", "server", "client", "json"],
    "homepage": "https://github.com/marcinsoja/php-json-rpc",
    "license": "MIT",
    "version": "0.9.1",
    "authors": [
        {
            "name": "Marcin Soja",
            "email": "dev@marcinsoja.pl",
            "homepage": "http://marcinsoja.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phing/phing": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-invoker": "*",
        "doctrine/annotations": "v1.1.1",
        "zendframework/zend-eventmanager": "2.2.0"
    },
    "suggest": {
        "doctrine/annotations": "Doctrine Annotations >=1.0 for annotation features",
        "zendframework/zend-eventmanager": "Zend EventManager for plugins/events features"
    },
    "autoload": {
        "psr-0": {
            "JsonRpcLib": "lib"
        }
    }
}